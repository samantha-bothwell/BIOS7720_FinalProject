library(reshape)
library(plyr)
### Remove subjects where exlusion criteria was met
cinci <- cinci[cinci$exclcrit == 0 | is.na(cinci$exclcrit),]
cinci <- cinci[!(cinci$record_id %in% c(8, 235, 237, 240, 137, 291, 189)),]
#### Check age of those with baseline creatinine vs without
cinci$admitage <- (as.Date(cinci$admissiondate, format = "%m/%d/%Y") - as.Date(cinci$dob, format = "%m/%d/%Y"))/365
cinci$admitage <- as.numeric(cinci$admitage)
cinci <- cinci[,-c(2,3)]
cinci$ph5 <- as.numeric(cinci$ph5)
cinci$cr1 <- as.numeric(cinci$cr1)
cinci$cr3 <- as.numeric(cinci$cr3)
cinci$cr6 <- as.numeric(cinci$cr6)
#### Summarize 7 day averages
cinci$cl.mean <- apply(cinci[,c(98:105)], 1, mean, na.rm = TRUE)
cinci$ph.mean <- apply(cinci[,c(122:129)], 1, mean, na.rm = TRUE)
cinci$cr.mean <- apply(cinci[,c(130:137)], 1, mean, na.rm = TRUE)
cinci$na.mean <- apply(cinci[,c(106:113)], 1, mean, na.rm = TRUE)
cinci$k.mean <- apply(cinci[,c(114:121)], 1, mean, na.rm = TRUE)
cinci$flbal.mean <- apply(cinci[,c(89:96)], 1, mean, na.rm = TRUE)
#### Missing Data Summary
explanatory = c("wt", "ht", "admitage", "gender", "cl.mean", "ph.mean",
"cr.mean", "na.mean", "k.mean", "flbal.mean")
dependent = "baselinecr"
cinci %>%
missing_compare(dependent, explanatory)
# Take out age < 1 yo
cinci %>%
filter(admitage >= 1) %>%
missing_compare(dependent, explanatory)
