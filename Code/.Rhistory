# Combine data
comb <- data.frame(Site = c(rep("Colorado", 253), rep("Cincinnati", 293)),
BCr = c(dem$baselinecr, cinci$baselinecr),
Age = c(dem$admitage, cinci$admitage),
Height = c(dem$height, cinci$ht))
comb$missbcr <- ifelse(is.na(comb$BCr), "Missing BCr", "Provided BCr")
# Plot
comb_summary = comb %>% group_by(Site, missbcr) %>%
summarize(median = median(Age, na.rm = T),
q1 = quantile(Age, 0.25), q3 = quantile(Age, 0.75)) %>%
mutate(lab = paste("Median = ", round(median,1), "\nQ1 = ", round(q1, 1), ", Q3 = ", round(q3, 1)))
ggplot(comb, aes(x = Age)) +
geom_histogram(fill = "turquoise4", col = "black", binwidth = 1) +
facet_grid(missbcr~Site) + ylab("Frequency") +
geom_text(data = comb_summary, aes(label = lab), x = 10, y = 45, size = 4)
# Change normative GFR
comb$calc.bcr <- ifelse(comb$Age < 1, comb$Height*0.55/90, comb$Height*0.55/120)
comb$calc.bcr.2 <- comb$Height*0.55/120
bcr1_sum = comb %>%
mutate(BCr1 = ifelse(is.na(BCr), calc.bcr, BCr), BCr2 = ifelse(is.na(BCr), calc.bcr.2, BCr)) %>%
group_by(Site, missbcr) %>%
summarize(median1 = median(BCr1, na.rm = T), median2 = median(BCr2, na.rm = T),
q1.1 = quantile(BCr, 0.25), q1.2 = quantile(BCr2, 0.25),
q3.1 = quantile(BCr1, 0.75), q3.2 = quantile(BCr2, 0.75)) %>%
mutate(lab1 = paste("Median = ", round(median1,1), "\nQ1 = ", round(q1.1, 1), ", Q3 = ", round(q3.1, 1)),
lab2 = paste("Median = ", round(median2,1), "\nQ1 = ", round(q1.2, 1), ", Q3 = ", round(q3.2, 1)))
